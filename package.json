{
  "name": "ignitegym",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "test": "jest --coverage",
    "test:badges": "npm run test && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings=0",
    "*.{json,html,css}": "prettier --write"
  },
  "dependencies": {
    "@testing-library/react-native": "^12.2.0",
    "expo": "~49.0.6",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@rocketseat/eslint-config": "^2.1.0",
    "@testing-library/jest-native": "^5.4.2",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.0.14",
    "eslint-plugin-testing-library": "^5.11.1",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "^49.0.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.2.3",
    "typescript": "^5.1.3"
  },
  "private": true
}
